		"prettier": "2.6.2",
		"prettier-plugin-svelte": "2.7.0",
		"reading-time": "1.5.0",
		"sass": "1.51.0",
		"svelte": "3.48.0",
		"svelte-check": "2.7.0",
		"svelte-preprocess": "4.10.6",
		"tslib": "2.4.0",
		"typescript": "4.6.4",
		"xml-escape": "1.1.0"
	},
	"type": "module",
	"dependencies": {
		"svelte-obfuscate": "0.3.0",
		"svelte-scroll-progress-bar": "0.3.0"
	}
